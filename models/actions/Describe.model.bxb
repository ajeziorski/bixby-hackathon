action (Describe) {
  description (The request for a more detailed description of an element in the scene)
  collect {
    input (objectName) {
      type (NamedObjectConcept)
      min (Required)
      default-init {
        intent {
          goal {NamedObjectConcept}
          value {
            NamedObjectConcept ("situation")
          }
        }
      }
    }
  }
  output (DescriptionResultConcept)
  type (Fetch)
}
